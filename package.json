    "cross-env": "^6.0.3",
    "datocms-structured-text-utils": "^1.0.1",
    "denodeify": "^1.2.1",
    "docopt": "^0.6.2",
    "dotenv": "^8.2.0",
    "escape-string-regexp": "^2.0.0",
